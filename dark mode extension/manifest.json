{
    "name": "Dark Mode Extension for Quercus",
    "description": "Turns Quercus page into dark mode or light mode",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "tabs", "activeTab", "scripting"]
}